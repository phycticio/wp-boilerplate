					"slug": "yellow"
				},
				{
					"gradient": "linear-gradient(135deg, #333333 25%, #003333 75%)",
					"name": "Charcoal",
					"slug": "charcoal"
				},
				{
					"gradient": "linear-gradient(135deg, #f5f5f5 25%, #c8c8c8 75%)",
					"name": "Cultured",
					"slug": "cultured"
				},
				{
					"gradient": "linear-gradient(135deg, #005F6B 25%, #00536b 75%)",
					"name": "Skobeloff",
					"slug": "skobeloff"
				}
			],
			"palette": [
				{
					"color": "#FBA419",
					"name": "Bright Yellow",
					"slug": "bright-yellow"
				},
				{
					"color": "#333333",
					"name": "Dark Charcoal",
					"slug": "dark-charcoal"
				},
				{
					"color": "#F5F5F5",
					"name": "Cultured",
					"slug": "cultured"
				},
				{
					"color": "#005F6B",
					"name": "Skobeloff",
					"slug": "skobeloff"
				}
			]
		},
		"layout": {
			"contentSize": "1260px",
			"wideSize": "1440px"
		},
		"spacing": {
			"units": [
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": true,
			"fontFamilies": [
				{

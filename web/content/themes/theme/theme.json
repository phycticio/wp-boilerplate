	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"color": "",
					"left": {
						"style": "none",
						"width": "0px"
					},
					"radius": "4px",
					"right": {
						"style": "none",
						"width": "0px"
					},
					"style": "",
					"top": {
						"style": "none",
						"width": "0px"
					},
					"width": ""
				},
				"variations": {
					"outline": {
						"color": {
							"text": "var(--wp--preset--color--cultured)"
						}
					}
				}
			},
			"core/calendar": {
				"color": {
					"background": "var(--wp--preset--color--cultured)",
					"text": "var(--wp--preset--color--dark-charcoal)"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--bright-yellow)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--bright-yellow)"
						}
					}
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--dark-charcoal)",
					"text": "var(--wp--preset--color--cultured)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--cultured)"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--30)",
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)",
						"top": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/file": {
				"border": {
					"radius": "4px"
				}
			},
			"core/gallery": {
				"color": {
					"background": "var(--wp--preset--color--cultured)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--20)",
						"right": "var(--wp--preset--spacing--20)",
						"top": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--cultured)"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--bright-yellow)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--bright-yellow)"
						}
					}
				}
			}
		},
		"color": {
			"gradient": "var(--wp--preset--gradient--charcoal)",
			"text": "var(--wp--preset--color--cultured)",
			"background": "var(--wp--preset--gradient--charcoal)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--bright-yellow)",
					"text": "var(--wp--preset--color--dark-charcoal)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--cultured)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--cultured)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lato)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--cultured)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--bright-yellow)"
				}
			}
		},
		"spacing": {
			"blockGap": "2rem",
			"padding": {
				"bottom": "0.5rem",
				"left": "0.5rem",
				"right": "0.5rem",
				"top": "0.5rem"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--lato)"
		}
	},
	"version": 3
}